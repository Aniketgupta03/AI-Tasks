%matplotlib inline
import matplotlib.pyplot as plt
plt.style.use('classic')
import numpy as np
x = ['France','USA','Yugoalavia','Romania','Argentina','Chile','Yugoslavia','USA','Uruguay']
y = [4,3,2,3,1,3,4,3,1]
plt.xlabel("Country")
plt.ylabel("Home team goals")
plt.title("Bar plot \n Country v/s Away team goals")
plt.bar(x,y, label='Bars1')
plt.legend()
plt.show()
